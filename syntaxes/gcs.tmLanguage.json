{
  "name": "GCS Script",
  "scopeName": "source.gcs",
  "fileTypes": ["gcs"],
  "patterns": [
    {
      "begin": "^(MOV|MOVREL|ANI|WAIT|SND|DO|LISTEN|SKIP)\\b",
      "beginCaptures": {
        "1": {
          "name": "keyword.control.gcs"
        }
      },
      "end": "$",
      "patterns": [
        {
          "match": "\\b[\\w_]+\\b",
          "name": "constant.other.gcs"
        },
        {
          "match": ",",
          "name": "punctuation.separator.gcs"
        }
      ]
    }
  ],
  "repository": {}
}